<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= book.category %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= book.content %>
  </p>

  <p>
    <strong>Synopsis:</strong>
    <%= book.synopsis %>
  </p>

  <p>
    <strong>Published:</strong>
    <%= book.published %>
  </p>

</div>
